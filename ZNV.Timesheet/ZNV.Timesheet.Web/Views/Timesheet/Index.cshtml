@using Abp.Web.Mvc.Extensions
@section styles {
    <link href="~/lib/AdminLTE-2.4.10/bower_components/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    <!-- iCheck for checkboxes and radio inputs -->
    <link rel="stylesheet" href="~/lib/AdminLTE-2.4.10/plugins/iCheck/all.css">
    <link href="~/css/editor.dataTables.min.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
}
@section scripts
{
    <!-- DataTables -->
    <script src="~/lib/AdminLTE-2.4.10/bower_components/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/lib/AdminLTE-2.4.10/bower_components/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <!-- iCheck 1.0.1 -->
    <script src="~/lib/AdminLTE-2.4.10/plugins/iCheck/icheck.min.js"></script>

    @Html.IncludeScript("~/js/Timesheet/Index.js")
}
<div class="row">
    <div class="col-xs-12">
        <!-- /.box -->
        <div class="box">
            <div class="box box-header">
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>我的周报</label>
                        </div>
                    </div>
                </div>
                <div class="btn-group-toggle pull-right">
                    <table cellpadding="0" cellspacing="0" width="100%">
                        <tr>
                            <td style="text-align:right">开始日期</td>
                            <td style="width:60px"><input id="startDate" width="100%" type="text" value="2019-06-01" /></td>
                            <td style="width:60px;text-align:right">结束日期</td>
                            <td style="width:60px"><input id="endDate" width="100%" type="text" value="2019-06-30" /></td>
                            <td style="width:80px;text-align:center">
                                <button type="button" name="search" id="search-btn" class="btn btn-success">
                                    查询 <i class="fa fa-search"></i>
                                </button>
                            </td>
                        </tr>
                    </table>
                </div>
                <div class="btn-group-toggle">
                    <button type="button" id="btCreate" class="btn btn-info">新增 <i class="fa fa-plus"></i></button>
                    <button type="button" id="btBulkDelete" class="btn btn-danger">批量删除 <i class="fa fa-remove"></i></button>
                </div>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <table id="dtTimesheet" class="table table-bordered table-striped table-hover">
                    <thead>
                        <tr>
                            <th>
                                <input type="checkbox" class="flat-red" checked>
                            </th>
                            <th>序号</th>
                            <th>日期</th>
                            <th>项目代码</th>
                            <th>项目组</th>
                            <th>工时</th>
                            <th>工作内容</th>
                            <th>备注</th>
                            <th>状态</th>
                            <th>审批人</th>
                            <th>审批时间</th>
                            <th></th>
                        </tr>
                    </thead>
                </table>
            </div>
            <!-- /.box-body -->
        </div>
        <!-- /.box -->
    </div>
    <!-- /.col -->
</div>
<div class="DTED_Lightbox_Background" style="opacity: 1;display:none"><div></div></div>
<div class="DTED_Lightbox_Wrapper" style="opacity: 1; top: 0px;display:none">
    <div class="DTED_Lightbox_Container">
        <div class="DTED_Lightbox_Content_Wrapper">
            <div class="DTED_Lightbox_Content" style="height: auto;">
                <div class="DTE DTE_Action_Create">
                    <div data-dte-e="head" class="DTE_Header">
                        <div class="DTE_Header_Content">
                            <font style="vertical-align: inherit;">
                                <font style="vertical-align: inherit;">创建新条目</font>
                            </font>
                        </div>
                    </div>
                    <div data-dte-e="processing" class="DTE_Processing_Indicator"><span></span></div>
                    <div data-dte-e="body" class="DTE_Body">
                        <div data-dte-e="body_content" class="DTE_Body_Content" style="max-height: 534px;">
                            <div data-dte-e="form_info" class="DTE_Form_Info" style="display: none;"></div>
                            <form data-dte-e="form" class="" style="display: block;">
                                <div data-dte-e="form_content" class="DTE_Form_Content">

                                </div>
                            </form>
                        </div>
                    </div>
                    <div data-dte-e="foot" class="DTE_Footer" style="text-indent: -1px;">
                        <div class="DTE_Footer_Content"></div>
                        <div data-dte-e="form_error" class="DTE_Form_Error" style="display: none;"></div>
                        <div data-dte-e="form_buttons" class="DTE_Form_Buttons">
                            <button class="btn" tabindex="0">
                                <font style="vertical-align: inherit;">
                                    <font style="vertical-align: inherit;">创建</font>
                                </font>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="DTED_Lightbox_Close"></div>
            </div>
        </div>
    </div>
</div>

